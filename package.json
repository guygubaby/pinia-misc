{
  "name": "pinia-misc",
  "type": "module",
  "version": "0.1.5",
  "packageManager": "pnpm@9.4.0",
  "description": "pinia related utilities",
  "author": "guygubaby <guygubaby@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/guygubaby",
  "homepage": "https://github.com/guygubaby/pinia-misc#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guygubaby/pinia-misc.git"
  },
  "bugs": "https://github.com/guygubaby/pinia-misc/issues",
  "keywords": [
    "pinia",
    "persist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "start": "esno src/index.ts",
    "test": "vitest",
    "play": "vite playground",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.7.2",
    "pinia": "^2.0.0",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "vue-demi": "^0.14.8"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@babel/types": "^7.24.7",
    "@bryce-loskie/eslint-config": "^2.2.0",
    "@types/node": "^20.14.10",
    "bumpp": "^9.4.1",
    "eslint": "^9.6.0",
    "esno": "^4.7.0",
    "happy-dom": "^14.12.3",
    "pinia": "^2.1.7",
    "pnpm": "^9.4.0",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0",
    "vite": "^5.3.3",
    "vitest": "^1.6.0",
    "vue": "^3.4.31"
  }
}
